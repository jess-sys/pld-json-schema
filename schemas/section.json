{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Section of Project Log Document,",
  "type": "object",
  "properties": {
    "type": {
      "const": "section"
    },
    "title": {
      "type": "string",
      "minLength": 1
    },
    "content": {
      "type": "array",
      "items": {
        "anyOf": [
          {
            "$ref": "#"
          },
          {
            "$ref": "./userStory.json"
          }
        ]
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "type",
    "title",
    "content"
  ]
}
